#include<stdio.h>
int main()
{
    int pass;
    int r_pass = 2002;

    while(pass != r_pass)
    {
        scanf("%d", &pass);

        if(pass != r_pass)
        {
            printf("Senha Invalida\n");
        }
        else if(pass == r_pass)
        {
            printf("Acesso Permitido\n");
            break;
        }
    }

    return 0;
}
