#include<stdio.h>
int main()
{
    int x, i;

    while(x != 0)
    {
        scanf("%d", &x);
        for(i = 1; i <= x; i++)
        {
            if(i != x)
                printf("%d ", i);
            else if(i == x)
                printf("%d\n", i);
        }

        if(x == 0)
            break;
        i++;
    }

    return 0;
}
